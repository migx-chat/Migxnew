{
  "expo": {
    "name": "migx",
    "slug": "migx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo_migx.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes",
      "bundleIdentifier": "com.migx.app",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },
    "android": {
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo_migx.png",
        "backgroundColor": "#043311"
      },
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.migx.app",
      "minSdkVersion": 24,
      "targetSdkVersion": 35,
      "compileSdkVersion": 35,
      "permissions": [
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_DATA_SYNC",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.REQUEST_IGNORE_BATTERY_OPTIMIZATIONS"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/logo_migx.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/logo_migx.png",
          "color": "#043311",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d02d63ab-ca34-4f57-a1e8-42c2c746ffc4"
      }
    }
  }
}
